config DEV_SDIO1_BOARD_VCC_VOLTAGE
    string "emmc card vcc voltage"
    default "SD_OPERATION_VOLTAGE_3V3"
    enum
        "SD_OPERATION_VOLTAGE_3V3"
        "SD_OPERATION_VOLTAGE_1V8"
        "SD_OPERATION_VOLTAGE_1V2"

