config DEV_SDIO1_MMC_HS200_1_2V
    bool "eMMC HS200 mode(1.2V I/O) is supported"
    default n


config DEV_SDIO1_MMC_HS400_1_8V
    bool "eMMC HS400 mode(1.8V I/O) is supported"
    default n


config DEV_SDIO1_MMC_HS400_1_2V
    bool "eMMC HS400 mode(1.2V I/O) is supported"
    default n


config DEV_SDIO1_BOARD_VCCQ_VOLTAGE
    string "emmc card vccq voltage"
    default "SD_OPERATION_VOLTAGE_3V3"
