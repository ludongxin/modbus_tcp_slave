source "D:/aworks_lp_hpm6450_general_sdk-0.9.1/frameworks/64cf32e2-aworks_lp/subsys/awb_common_driver/dts/bindings/buzzer/awb_pwm_buzzer.Kconfig.opts"

endif #DRV_PWM_BUZZER
