
menuconfig AW_APP_AW_SHELL_MTD
    bool "AWorks Shell supports MTD commands"
    libsafe 
    select AW_APP_SHELL
    select AW_INTERFACE
    select AW_STDLIB
    select AW_DEV_API
    select AW_STDIO_PRINTF_FLAGS_MINUS_EN
    select AW_STDIO_PRINTF_TYPES_LONG_LONG_EN
    default n
    help
        Version: 0.0.0

        
